{
  "name": "@pangea/price-oracle",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "files": [
    "artifacts",
    "contracts",
    "deploy",
    "deployments",
    "exports",
    "types"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "TS_NODE_TRANSPILE_ONLY=1 hardhat compile --network hardhat",
    "clean": "hardhat clean",
    "hardhat:deploy": "hardhat node --network hardhat",
    "baobab:deploy": "hardhat --network baobab deploy",
    "baobab:export": "hardhat --network baobab export",
    "cypress:deploy": "hardhat --network cypress deploy",
    "cypress:export": "hardhat --network cypress export",
    "test": "hardhat test --network hardhat",
    "test:parallel": "hardhat test --network hardhat --parallel",
    "test:trace": "hardhat test --logs --network hardhat",
    "prettier": "prettier --write 'test/*.{js,ts}' && prettier --write contracts/**/*.sol",
    "lint": "yarn prettier && solhint -c .solhint.json contracts/**/*.sol",
    "size": "hardhat compile && hardhat size-contracts",
    "doc": "hardhat dodoc"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ],
    "*.sol": [
      "prettier --write"
    ],
    "*.md": "prettier --write",
    "*.{js,css,md}": "prettier --write"
  },
  "devDependencies": {
    "@codechecks/client": "^0.1.12",
    "@defi-wonderland/smock": "^2.0.7",
    "@ethersproject/solidity": "^5.5.0",
    "@nomiclabs/hardhat-ethers": "https://registry.npmjs.org/hardhat-deploy-ethers/-/hardhat-deploy-ethers-0.3.0-beta.13.tgz",
    "@nomiclabs/hardhat-etherscan": "^3.0.1",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.4.1",
    "@openzeppelin/contracts-upgradeable": "^4.5.2",
    "@primitivefi/hardhat-dodoc": "^0.2.3",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.3",
    "hardhat": "^2.8.4",
    "hardhat-abi-exporter": "^2.8.0",
    "hardhat-contract-sizer": "^2.3.0",
    "hardhat-deploy": "^0.10.5",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.6",
    "hardhat-interface-generator": "^0.0.6",
    "hardhat-log-remover": "^2.0.2",
    "hardhat-preprocessor": "^0.1.4",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-tracer": "^1.0.0-alpha.6",
    "hardhat-watcher": "^2.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^6.1.0",
    "typescript": "^4.6.4",
    "witnet-solidity-bridge": "^0.5.4"
  }
}
